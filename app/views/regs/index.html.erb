<p id="notice"><%= notice %></p>

<h1>Regs</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Dateofbirth</th>
      <th>Email</th>
      <th>Telephone</th>
      <th>Address</th>
      <th>City</th>
      <th>Postcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regs.each do |reg| %>
      <tr>
        <td><%= reg.username %></td>
        <td><%= reg.password %></td>
        <td><%= reg.dateofbirth %></td>
        <td><%= reg.email %></td>
        <td><%= reg.telephone %></td>
        <td><%= reg.address %></td>
        <td><%= reg.city %></td>
        <td><%= reg.postcode %></td>
        <td><%= link_to 'Show', reg %></td>
        <td><%= link_to 'Edit', edit_reg_path(reg) %></td>
        <td><%= link_to 'Destroy', reg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reg', new_reg_path %>
